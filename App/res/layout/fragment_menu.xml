<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0e4fc"
    tools:context=".activities.MainActivity">


    <Button
        android:id="@+id/botonmapa"
        android:layout_width="378dp"
        android:layout_height="115dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/botones_redondos"
        android:backgroundTint="#9FBB86FC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ib_mapa"
        android:layout_width="57dp"
        android:layout_height="64dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#9FBB86FC"
        android:contentDescription="@string/mapaimg"
        android:elevation="3dp"
        android:src="@drawable/mapa"
        app:layout_constraintStart_toStartOf="@+id/botonmapa"
        app:layout_constraintTop_toTopOf="@+id/botonmapa"
        tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck,DuplicateSpeakableTextCheck" />

    <TextView
        android:id="@+id/txContacto"
        android:layout_width="99dp"
        android:layout_height="48dp"
        android:textAlignment="center"
        android:layout_marginTop="32dp"
        android:elevation="3dp"
        android:letterSpacing="0.1"
        android:text="@string/mapatxt"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/botonmapa"
        app:layout_constraintStart_toStartOf="@+id/botonmapa"
        app:layout_constraintTop_toTopOf="@+id/botonmapa"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/botonmedicaments"
        android:layout_width="378dp"
        android:layout_height="115dp"
        android:layout_marginTop="88dp"
        android:background="@drawable/botones_redondos"
        android:backgroundTint="#9FBB86FC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonmapa"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ib_medicamnets"
        android:layout_width="59dp"
        android:layout_height="65dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#9FBB86FC"
        android:contentDescription="@string/todotxt"
        android:elevation="3dp"
        android:src="@drawable/medicacio"
        app:layout_constraintStart_toStartOf="@+id/botonmedicaments"
        app:layout_constraintTop_toTopOf="@+id/botonmedicaments"
        tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck" />

    <TextView
        android:id="@+id/txt_Dosis"
        android:layout_width="239dp"
        android:layout_height="46dp"
        android:layout_marginTop="32dp"
        android:elevation="3dp"
        android:letterSpacing="0.1"
        android:textAlignment="center"
        android:text="@string/medicamentstxt"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/botonmedicaments"
        app:layout_constraintStart_toStartOf="@+id/botonmedicaments"
        app:layout_constraintTop_toTopOf="@+id/botonmedicaments" />

    <Button
        android:id="@+id/botonagenda"
        android:layout_width="378dp"
        android:layout_height="115dp"
        android:layout_marginTop="88dp"
        android:background="@drawable/botones_redondos"
        android:backgroundTint="#9FBB86FC"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonmedicaments"
        tools:ignore="SpeakableTextPresentCheck"
        tools:visibility="visible" />

    <ImageButton
        android:id="@+id/ib_agenda"
        android:layout_width="56dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#9FBB86FC"
        android:contentDescription="@string/todotxtt"
        android:elevation="3dp"
        android:src="@drawable/agenda"
        app:layout_constraintStart_toStartOf="@+id/botonagenda"
        app:layout_constraintTop_toTopOf="@+id/botonagenda"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/texto_menÃº4"
        android:layout_width="144dp"
        android:layout_height="57dp"
        android:layout_marginTop="32dp"
        android:elevation="3dp"
        android:textAlignment="center"
        android:letterSpacing="0.1"
        android:text="@string/agendatxt"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/botonagenda"
        app:layout_constraintStart_toStartOf="@+id/botonagenda"
        app:layout_constraintTop_toTopOf="@+id/botonagenda" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar4"
        android:layout_width="412dp"
        android:layout_height="71dp"
        android:layout_marginTop="52dp"
        android:background="#9FBB86FC"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonagenda"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/txContactanos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="168dp"
        android:layout_marginTop="4dp"
        android:text="@string/contactanostxt"
        android:textColor="#000000"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar4" />

    <TextView
        android:id="@+id/txContTlf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="60dp"
        android:text="@string/contacttlftxt"
        android:textColor="#000000"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/toolbar4"
        app:layout_constraintTop_toBottomOf="@+id/txContactanos" />

    <TextView
        android:id="@+id/txContEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:text="@string/contactemailtxt"
        android:textColor="#000000"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txContactanos" />


</androidx.constraintlayout.widget.ConstraintLayout>